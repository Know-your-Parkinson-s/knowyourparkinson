{
  "name": "sass-builder",
  "version": "1.0.0",
  "description": "",
  "main": "sass/freelancer.scss",
  "scripts": {
    "css-build-bootstrap": "node-sass --omit-source-map-url sass/mybootstrap.scss css/mybootstrap.css",
